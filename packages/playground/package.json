{
    "name": "@kehwar/playground",
    "type": "module",
    "version": "0.0.11",
    "private": "true",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./dist/index.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "dev": "unbuild --stub",
        "lint": "eslint .",
        "lint:config": "eslint-flat-config-viewer",
        "prepublishOnly": "build",
        "start": "esno src/index.ts",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@kehwar/toolbox": "^0.0.2"
    },
    "devDependencies": {
        "@antfu/ni": "^0.21.12",
        "@types/node": "^20.10.6",
        "esno": "^4.0.0",
        "rimraf": "^5.0.5",
        "typescript": "^5.3.3",
        "unbuild": "^2.0.0",
        "vite": "^5.0.10",
        "vitest": "^1.1.2"
    }
}
