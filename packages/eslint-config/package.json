{
    "name": "@kehwar/eslint-config",
    "type": "module",
    "version": "0.0.0",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./dist/index.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "dev": "unbuild --stub",
        "lint": "eslint .",
        "start": "esno src/index.ts",
        "typecheck": "tsc --noEmit"
    },
    "peerDependencies": {
        "eslint": "^8.53.0"
    },
    "dependencies": {
        "@antfu/eslint-config": "2.6.1",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@antfu/ni": "^0.21.12",
        "@antfu/utils": "^0.7.7",
        "@types/lodash": "^4.14.202",
        "@types/node": "^20.10.6",
        "eslint": "^8.56.0",
        "esno": "^4.0.0",
        "pnpm": "^8.14.0",
        "rimraf": "^5.0.5",
        "typescript": "^5.3.3",
        "unbuild": "^2.0.0",
        "vite": "^5.0.10",
        "vitest": "^1.1.2"
    }
}
