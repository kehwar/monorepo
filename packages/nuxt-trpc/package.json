{
    "name": "@kehwar/nuxt-trpc",
    "type": "module",
    "version": "0.0.16",
    "description": "TRPC wrapper and remote functions",
    "license": "MIT",
    "repository": "kehwar/nuxt-trpc",
    "exports": {
        ".": {
            "types": "./dist/types.d.ts",
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
        "*.d.ts",
        "dist"
    ],
    "scripts": {
        "prepack": "nuxt-module-build build",
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
        "release": "pnpm run prepack && pnpm publish && git push --follow-tags",
        "lint": "eslint .",
        "test": "vitest run",
        "test:watch": "vitest watch"
    },
    "peerDependencies": {
        "@trpc/client": "^10.44.1",
        "@trpc/server": "^10.44.1",
        "devalue": "^4.3.2",
        "superjson": "^2.2.1",
        "trpc-nuxt": "^0.10.12"
    },
    "dependencies": {
        "@nuxt/kit": "^3.9.3",
        "@rollup/pluginutils": "^5.1.0",
        "dedent": "^1.5.1",
        "es-module-lexer": "^1.4.1",
        "fast-glob": "^3.3.2",
        "lodash": "^4.17.21",
        "magicast": "^0.3.3",
        "minimatch": "^9.0.3",
        "p-map": "^7.0.1",
        "pathe": "^1.1.2",
        "vite": "^5.0.12"
    },
    "devDependencies": {
        "@kehwar/eslint-config": "workspace:*",
        "@nuxt/devtools": "^1.0.8",
        "@nuxt/eslint-config": "^0.2.0",
        "@nuxt/module-builder": "^0.5.5",
        "@nuxt/schema": "^3.9.3",
        "@nuxt/test-utils": "^3.10.0",
        "@trpc/client": "^10.45.0",
        "@trpc/server": "^10.45.0",
        "@types/lodash": "^4.14.202",
        "@types/node": "^20.11.7",
        "bumpp": "^9.3.0",
        "changelogen": "^0.5.5",
        "devalue": "^4.3.2",
        "eslint": "^8.56.0",
        "nuxt": "^3.9.3",
        "superjson": "^2.2.1",
        "trpc-nuxt": "^0.10.17",
        "type-fest": "^4.10.1",
        "vitest": "^1.2.2"
    }
}
